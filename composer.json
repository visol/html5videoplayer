      "role": "Developer",
      "homepage": "https://github.com/lochmueller"
    }
  ],
  "require": {
    "php": ">=5.4.0",
    "typo3/cms-core": "~6.2.14|>=7.4.0,<8.0"
  },
  "replace": {
    "html5videoplayer": "self.version",
    "typo3-ter/html5videoplayer": "self.version"
